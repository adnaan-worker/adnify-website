erve(entry.target);\r\n      }\r\n    });\r\n  }, observerOptions);\r\n\r\n  // Observe all animated elements\r\n  document.querySelectorAll('.feature-block, .bento-card, .stat-item, .section-title, .section-subtitle, .contributor-card, .contributors-grid, .contribute-cta').forEach(el => {\r\n    observer.observe(el);\r\n  });\r\n\r\n  // Fallback for elements already in viewport\r\n  setTimeout(() => {\r\n    document.querySelectorAll('.feature-block, .bento-card, .stat-item').forEach(el => {\r\n      const rect = el.getBoundingClientRect();\r\n      const inViewport = rect.top < window.innerHeight && rect.bottom > 0;\r\n      if (inViewport && !el.classList.contains('visible')) {\r\n        el.classList.add('visible');\r\n      }\r\n    });\r\n  }, 100);\r\n}","startLine":94,"endLine":144,"type":"function","language":"javascript","symbols":["initScrollAnimations"],"termFreq":[["function",1],["initscrollanimations",1],["const",6],["observeroptions",2],["threshold",1],["config",2],["animation",3],["scrollthreshold",1],["rootmargin",1],["scrollmargin",1],["observer",3],["new",1],["intersectionobserver",1],["entries",2],["foreach",4],["entry",8],["if",5],["isintersecting",1],["target",6],["classlist",6],["add",3],["visible",5],["animate",1],["numbers",1],["when",1],["stat",5],["item",7],["becomes",1],["contains",3],["numel",5],["queryselector",1],["number",1],["dataset",2],["animated",3],["animatenumber",1],["true",1],["handle",1],["contributor",3],["items",3],["staggered",1],["contributors",2],["grid",2],["queryselectorall",3],["index",2],["settimeout",2],["unobserve",1],["observe",2],["all",1],["elements",2],["document",2],["feature",2],["block",2],["bento",2],["card",3],["section",2],["title",1],["subtitle",1],["contribute",1],["cta",1],["el",6],["fallback",1],["for",1],["already",1],["in",1],["viewport",1],["rect",3],["getboundingclientrect",1],["inviewport",2],["top",1],["window",1],["innerheight",1],["bottom",1]],"docLength":160},{"id":"E:\\Project\\website\\ass